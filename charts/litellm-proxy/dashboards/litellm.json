{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum(rate(litellm_proxy_total_requests_metric{namespace=\"$namespace\"}[5m])) by (model)",
          "legendFormat": "{{ model }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "title": "Request Latency (p50/p95/p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(litellm_request_total_latency_metric_bucket{namespace=\"$namespace\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(litellm_request_total_latency_metric_bucket{namespace=\"$namespace\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(litellm_request_total_latency_metric_bucket{namespace=\"$namespace\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "title": "Token Consumption",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum(rate(litellm_input_tokens_metric{namespace=\"$namespace\"}[5m])) by (model)",
          "legendFormat": "Input - {{ model }}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(litellm_output_tokens_metric{namespace=\"$namespace\"}[5m])) by (model)",
          "legendFormat": "Output - {{ model }}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "title": "Time to First Token (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(litellm_llm_api_time_to_first_token_metric_bucket{namespace=\"$namespace\"}[5m])) by (le, model))",
          "legendFormat": "{{ model }} p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "title": "Per-Team Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum(rate(litellm_proxy_total_requests_metric{namespace=\"$namespace\"}[5m])) by (team)",
          "legendFormat": "{{ team }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "title": "Error Rate by Model",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum(rate(litellm_proxy_total_requests_metric{namespace=\"$namespace\", status!=\"200\"}[5m])) by (model, status)",
          "legendFormat": "{{ model }} ({{ status }})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["litellm", "llm-platform"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false
      },
      {
        "name": "namespace",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": "label_values(litellm_proxy_total_requests_metric, namespace)",
        "current": { "text": "All", "value": "$__all" },
        "includeAll": true,
        "hide": 0,
        "multi": false,
        "refresh": 2
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "LiteLLM Proxy",
  "uid": "litellm-proxy-overview",
  "version": 1
}
